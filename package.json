{
  "name": "taomu-ui",
  "private": false,
  "version": "0.0.5",
  "type": "module",
  "main": "index.js",
  "unpkg": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "*"
  ],
  "scripts": {
    "pu": "npm run build && npm publish ./dist",
    "dev": "npm run doc",
    "doc": "npm run storybook",
    "build": "node scripts/rm-dist.js && tsc -p tsconfig.lib.json && node scripts/copy-file.js",
    "build:typedoc": "typedoc --tsconfig tsconfig.lib.json",
    "build:docs": "npm run build-storybook && npm run build:typedoc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "atomic-cls": "^0.0.6",
    "clsx": "^2.1.1",
    "react-feather": "^2.0.10",
    "react-mount-node": "^0.0.1",
    "taomu-store": "^0.0.1",
    "taomu-toolkit": "^0.0.9"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.2.9",
    "typedoc": "^0.26.7",
    "typescript": "^5.5.4",
    "vite": "^5.4.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}